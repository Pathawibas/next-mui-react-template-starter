{
  "extends": [
    "next/core-web-vitals",
    "plugin:@next/next/recommended",
    "prettier"
  ],
  "plugins": ["material-ui"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "import/extensions": "off",
    "semi": "off",
    "arrow-body-style": "off",
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["@mui/*/*/*"]
      }
    ]
  }
}
